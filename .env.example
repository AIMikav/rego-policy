# LLM Provider Configuration
# Choose one: llamastack, anthropic, openai, google
LLM_PROVIDER=google
LLM_TEMPERATURE=0.1
LLM_MAX_TOKENS=1024

# Anthropic Configuration (if using anthropic provider)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-haiku-20240307

# Google gemini configuration
GOOGLE_API_KEY=your_google_api_here
GOOGLE_MODEL=gemini-2.0-flash-001

# OpenAI Configuration (if using openai provider)
OPENAI_API_KEY=your_openai_api_here

# RAG Configuration
RAG_CHUNK_SIZE=512
RAG_CHUNK_OVERLAP=50
RAG_TOP_K=3

# Policy Configuration
POLICY_DIR=data

# Debug
DEBUG=true
