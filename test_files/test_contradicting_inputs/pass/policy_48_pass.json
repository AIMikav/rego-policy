{
  "input": {
    "review": {
      "object": {
        "apiVersion": "apps/v1",
        "kind": "Deployment",
        "metadata": {
          "name": "app-deployment",
          "namespace": "default"
        },
        "spec": {
          "replicas": 1,
          "template": {
            "metadata": {
              "labels": {
                "app": "my-app",
                "version": "v1"
              }
            },
            "spec": {
              "containers": [
                {
                  "name": "app",
                  "image": "nginx:1.20"
                }
              ]
            }
          }
        },
        "items": [
          {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
              "name": "app-service",
              "namespace": "default"
            },
            "spec": {
              "selector": {
                "app": "my-app",
                "version": "v1"
              },
              "ports": [
                {
                  "port": 80,
                  "targetPort": 8080
                }
              ]
            }
          }
        ]
      }
    }
  }
}